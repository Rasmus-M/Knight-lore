       movb r1, *r0
       movb r1, *r0
       movb r1, *r0
       movb r1, *r0
       movb r1, *r0
       movb r1, *r0
       dec  r3
       jne  vdpwr1
       andi r2, >0007                   ; Isolate number of remaining bytes
       jeq  vdpwr3
vdpwr2 movb r1, *r0
       dec  r2
       jne  vdpwr2
vdpwr3 b    *r11
